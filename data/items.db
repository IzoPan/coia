":"","amount":null},"ability":"","actionType":"msak","attackBonus":"5","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":13,"scaling":"flat"},"consumableType":"scroll"},"effects":[],"ownership":{"default":0,"EkBjaHUIGZ1pQeoq":3},"flags":{"core":{"sourceId":"Compendium.dnd5e.items.XdDp6CKh9qEvPTuS"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.4","coreVersion":"10.291","createdTime":1661787233703,"modifiedTime":1676708872708,"lastModifiedBy":"EkBjaHUIGZ1pQeoq"},"folder":"7Ucyq7XVTTMS2EKB","sort":5800000,"_id":"RvxNaRjZwmEUBNJ9"}
