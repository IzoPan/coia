ter\">\n    <span>1st Level</span>\n    <span>V</span>\n    <span>1 Reaction (a creature you can see within 60 feet of yourself succeeds on an attack roll, an ability check, or a saving throw)</span>\n    <span>1 Creature</span>\n    <span>60 Feet</span>\n    <span>Instantaneous</span>\n  </footer>\n</div>","speaker":{"scene":"UJ2sgV1nwOunYwVa","token":"uxa2noxl5ortgb8v","actor":"VyAUzoC0D4Ah9xqf","alias":"Teolth Tizoc"},"whisper":[],"blind":false,"emote":false,"flags":{"midi-qol":{"itemUuid":"Actor.VyAUzoC0D4Ah9xqf.Item.36IlQHgdKJ7plgh0","actorUuid":"Actor.VyAUzoC0D4Ah9xqf","sound":"","type":0,"itemId":"36IlQHgdKJ7plgh0","workflowId":"Actor.VyAUzoC0D4Ah9xqf.Item.36IlQHgdKJ7plgh0"},"core":{"canPopout":true}},"rolls":[],"sound":null}
