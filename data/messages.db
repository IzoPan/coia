ter\">\n    <span>1st Level</span>\n    <span>V</span>\n    <span>1 Reaction (a creature you can see within 60 feet of yourself succeeds on an attack roll, an ability check, or a saving throw)</span>\n    <span>1 Creature</span>\n    <span>60 Feet</span>\n    <span>Instantaneous</span>\n  </footer>\n</div>","speaker":{"scene":"TMNbta9k7kkJRoiv","token":"gdgocc2e8mld07jo","actor":"AQexnBOSpaJojOyY","alias":"Madre"},"whisper":[],"blind":false,"emote":false,"flags":{"midi-qol":{"itemUuid":"Actor.AQexnBOSpaJojOyY.Item.zFf8gfb1qKzaHXqe","actorUuid":"Actor.AQexnBOSpaJojOyY","sound":"","type":0,"itemId":"zFf8gfb1qKzaHXqe","workflowId":"Actor.AQexnBOSpaJojOyY.Item.zFf8gfb1qKzaHXqe"},"core":{"canPopout":true}},"rolls":[],"sound":null}
