","quantity":1,"weight":9,"price":{"value":20,"denomination":"gp"},"attunement":0,"equipped":false,"rarity":"common","identified":true,"toolType":"art","baseItem":"brewer","ability":"wis","chatFlavor":"","proficient":0,"bonus":""},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"flags":{"monks-enhanced-journal":{"parentId":"Y9S75go1hLMXUD48","price":"20 gp","cost":"20 gp","quantity":1,"from":""}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.0","coreVersion":"10.291","createdTime":1661787233704,"modifiedTime":1671220961611,"lastModifiedBy":"dnd5ebuilder0000"}},{"_id":"S58VvQU7CQ1V5Tu4","name":"Vial","type":"backpack","img":"icons/consumables/potions/bottle-bulb-empty-glass.webp","system":{"description":{"value":"<p>A vial can hold up to 4 ounces of liquid.</p>","chat":"","unidentified":""},"source":"SRD 5.1","quantity":1,"weight":0,"price":{"value":1,"denomination":"gp"},"attunement":0,"equipped":false,"rarity":"common","identified":true,"capacity":{"type":"weight","value":0.25,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0}},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"flags":{"monks-enhanced-journal":{"parentId":"meJEfX3gZgtMX4x2","price":"1 gp","cost":"1 gp","quantity":1,"from":""}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.0","coreVersion":"10.291","createdTime":1661787233854,"modifiedTime":1671220963991,"lastModifiedBy":"dnd5ebuilder0000"}}],"actors":["gGhfDzIvryaHuwBD","tWdrWv89qXjOk60i","Do0LwuTFr8I0CNVE","drwS9H2AwxzqiitW","7t23vJAGPi5QsNtz"],"scrollPos":"{}","currency":{"pp":0,"gp":0,"sp":0,"cp":0,"ep":0},"appendix":false,"lastrolltable":"Compendium.monks-shops.shop-items.H2VL0s4w9G14JKFJ"}},"_id":"h5Cl9Nayc7PyuSpc","title":{"show":true,"level":1},"image":{},"text":{"format":1},"video":{"controls":true,"volume":0.5},"src":"modules/monks-enhanced-journal/assets/loot.png","system":{},"sort":0,"ownership":{"default":-1}}],"sort":0,"ownership":{"default":2,"EkBjaHUIGZ1pQeoq":3},"_stats":{"systemId"