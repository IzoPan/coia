/tools/laboratory/cauldron-filled-gold.webp","system":{"description":{"value":"<p>An iron pot holds 1 gallon of liquid.</p>","chat":"","unidentified":""},"source":"SRD 5.1","quantity":1,"weight":10,"price":{"value":2,"denomination":"gp"},"attunement":0,"equipped":false,"rarity":"common","identified":true,"capacity":{"type":"weight","value":8.4,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0}},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"flags":{"monks-enhanced-journal":{"parentId":"M8xM8BLK4tpUayEE","price":"2 gp","cost":"2 gp","quantity":1,"consumable":false,"lock":false,"hide":false}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.0","coreVersion":"10.291","createdTime":1661787233623,"modifiedTime":1671220960143,"lastModifiedBy":"dnd5ebuilder0000"}},{"_id":"kYxsRI0sHuL64st2","name":"Hammer","type":"loot","img":"icons/tools/hand/hammer-cobbler-steel.webp","system":{"description":{"value":"<p>A tool with a heavy metal head mounted at the end of its handle, used for jobs such as breaking things and driving in nails. </p>","chat":"","unidentified":""},"source":"SRD 5.1","quantity":1,"weight":3,"price":{"value":1,"denomination":"gp"},"rarity":"common","identified":true},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"flags":{"monks-enhanced-journal":{"parentId":"14pNRT4sZy9rgvhb","price":"1 gp","cost":"1 gp","quantity":1,"consumable":false,"lock":false,"hide":false}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.0","coreVersion":"10.291","createdTime":1661787233437,"modifiedTime":1671220957013,"lastModifiedBy":"dnd5ebuilder0000"}}],"sell":1,"buy":0.5,"_lasttab":"items","lastrolltable":"Compendium.item-piles.item-piles-merchant-tables.uytSQbHPZU4zgVHt","scrollPos":"{\".shop-items > .item-list\":464,\".tab.description .tab-inner\":0}","shoptype":"","location":"","EkBjaHUIGZ1pQeoq":{"notes":""},"appendix":false}},"_id":"b6GhIfECe5klgj50","title":{"show":true,"level":1},"image":{},"text":{"format":1},"video":{"cont